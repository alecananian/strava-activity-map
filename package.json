{
  "name": "strava-activity-map",
  "version": "1.3.0",
  "description": "Map visualizations for Strava activities",
  "main": "./src/index.js",
  "scripts": {
    "analyze": "source-map-explorer --no-border-checks 'dist/*.js'",
    "start": "parcel ./src/index.html",
    "build": "parcel build ./src/index.html",
    "build:gh": "npm run build -- --public-url /strava-activity-map",
    "lint": "eslint ./src --ext .tsx,.ts",
    "test": "jest ./src --coverage"
  },
  "keywords": [
    "strava",
    "visualization",
    "fitness"
  ],
  "author": "Alec Ananian",
  "license": "MIT",
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/styles": "^4.10.0",
    "axios": "^0.19.2",
    "dayjs": "^1.8.32",
    "i18next": "^19.6.3",
    "i18next-browser-languagedetector": "^5.0.1",
    "leaflet": "^1.6.0",
    "polyline-encoded": "0.0.9",
    "react": "^16.13.1",
    "react-color": "^2.18.1",
    "react-dom": "^16.13.1",
    "react-i18next": "^11.7.0",
    "react-leaflet": "^2.7.0",
    "styled-components": "^5.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "@types/jest": "^26.0.9",
    "@types/node": "^14.0.27",
    "@types/react": "^16.9.45",
    "@types/react-color": "^3.0.4",
    "@types/react-dom": "^16.9.8",
    "@types/react-leaflet": "^2.5.2",
    "@types/styled-components": "^5.1.2",
    "@typescript-eslint/eslint-plugin": "^3.8.0",
    "@typescript-eslint/parser": "^3.8.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "eslint": "^7.6.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-import-resolver-parcel": "^1.10.4",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.5",
    "husky": "^4.2.5",
    "jest": "^26.2.2",
    "lint-staged": "^10.2.11",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-static-files-copy": "^2.4.4",
    "source-map-explorer": "^2.4.2",
    "ts-jest": "^26.1.4",
    "typescript": "^3.9.7"
  },
  "browserslist": [
    "defaults",
    "not IE 11",
    "not IE_Mob 11",
    "maintained node versions"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{tsx,ts}": "npm run lint -- --fix"
  }
}
