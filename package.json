{
  "name": "strava-activity-map",
  "version": "1.4.0",
  "description": "Map visualizations for Strava activities",
  "main": "./src/index.js",
  "scripts": {
    "analyze": "source-map-explorer --no-border-checks 'dist/*.js'",
    "start": "parcel ./src/index.html",
    "build": "parcel build ./src/index.html",
    "build:gh": "npm run build -- --public-url /strava-activity-map",
    "lint": "eslint ./src --ext .tsx,.ts",
    "test": "jest ./src"
  },
  "keywords": [
    "strava",
    "visualization",
    "fitness"
  ],
  "author": "Alec Ananian",
  "license": "MIT",
  "dependencies": {
    "@material-ui/core": "^4.10.2",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/styles": "^4.10.0",
    "axios": "^0.21.1",
    "dayjs": "^1.8.28",
    "i18next": "^20.4.0",
    "i18next-browser-languagedetector": "^6.1.2",
    "leaflet": "^1.6.0",
    "polyline-encoded": "0.0.9",
    "react": "^17.0.2",
    "react-color": "^2.18.1",
    "react-dom": "^17.0.2",
    "react-i18next": "^11.5.1",
    "react-leaflet": "^2.7.0",
    "styled-components": "^5.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-react": "^7.10.1",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.1",
    "@types/react": "^17.0.19",
    "@types/react-color": "^3.0.2",
    "@types/react-dom": "^17.0.9",
    "@types/react-leaflet": "^2.5.1",
    "@types/styled-components": "^5.1.0",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "eslint": "^7.2.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-import-resolver-parcel": "^1.10.4",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.20.0",
    "husky": "^4.2.5",
    "lint-staged": "^11.1.2",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-static-files-copy": "^2.4.3",
    "typescript": "^4.3.5"
  },
  "browserslist": [
    "defaults",
    "not IE 11",
    "not IE_Mob 11",
    "maintained node versions"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm test"
    }
  },
  "lint-staged": {
    "*.{tsx,ts}": "npm run lint -- --fix"
  }
}
